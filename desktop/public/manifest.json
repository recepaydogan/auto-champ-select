{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Auto Champ Select",
    "version": "0.0.1",
    "minimum-overwolf-version": "0.195",
    "author": "Antigravity",
    "icon": "icon.png",
    "icon_gray": "icon_gray.png",
    "launcher_icon": "icon.png",
    "description": "Remote accept and champ select for League of Legends"
  },
  "permissions": [
    "GameInfo",
    "FileSystem"
  ],
  "data": {
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000
    },
    "start_window": "background",
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        5426
      ]
    },
    "windows": {
      "background": {
        "file": "background.html",
        "background_optimization": false,
        "is_background_page": true
      },
      "desktop": {
        "file": "index.html",
        "transparent": true,
        "desktop_only": true,
        "resizable": false,
        "size": {
          "width": 1200,
          "height": 800
        },
        "min_size": {
          "width": 1200,
          "height": 800
        },
        "disable_auto_dpi_sizing": false
      }
    },
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            5426
          ]
        },
        "start_minimized": false
      }
    ]
  }
}